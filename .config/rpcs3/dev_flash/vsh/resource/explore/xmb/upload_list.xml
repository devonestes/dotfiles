	<View id="upload_hdd_all_root">
		<Items>
			<Query
				class="type:x-xmb/folder-pixmap"
				key="upload_video_hdd"
				src="#upload_video_hdd"
			/>
			<Query
				class="type:x-xmb/folder-pixmap"
				key="upload_music_hdd"
				src="#upload_music_hdd"
			/>
			<Query
				class="type:x-xmb/folder-pixmap"
				key="upload_photo_hdd"
				src="#upload_photo_hdd"
			/>
		</Items>
	</View>


</XMBML>
